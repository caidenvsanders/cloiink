{
  "name": "cloink.net",
  "version": "0.0.0",
  "description": "cloink.net",
  "repository": "https://github.com/caidenvsanders/cloink.net",
  "author": "Caiden Sanders",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format": "prettier --write packages/**/src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint packages/**/src/**/*.ts{,x}",
    "WebFrontendTestCI": "yarn --cwd packages/web-frontend test:ci",
    "APITestCI": "yarn concurrently --kill-others --success first \"yarn --cwd packages/api dev\" \"wait-on tcp:4000 && yarn --cwd packages/api test:ci\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.1"
  },
  "dependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.0"
  }
}
