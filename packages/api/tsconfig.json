{
	"compilerOptions": {
	  "outDir": "./dist",
	  "module": "commonjs",
	  "target": "es6",
	  "lib": ["es6", "dom"],
	  "sourceMap": false,
	  "allowJs": true,
	  "moduleResolution": "node",
	  "baseUrl": "src",
	  "noImplicitReturns": true,
	  "noImplicitThis": true,
	  "noImplicitAny": true,
	  "strictNullChecks": true,
	  "esModuleInterop": true,
	  "allowSyntheticDefaultImports": true,
	  "jsx": "react",
	  "types": ["node", "jest"],
	},
	"include": ["src"],
	"exclude": ["src/__tests__"]
  }